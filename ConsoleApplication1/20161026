using System.Threading.Tasks;

namespace ConsoleApplication1
{
    class Program
    {

        

        static void Main(string[] args)
        {
            ZhuangBi.classBegin();
            ZhuangBi.classOver();
        }
    }

    class ZhuangBi
    {
        public ZhuangBi()
        {

        }

        public static void classBegin()
        {
            Console.WriteLine("上课");
        }

        public static void classOver()
        {
            Console.WriteLine("下课");
        }
    }

}
